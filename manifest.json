{
	"manifest_version": 3,
	"name": "NYT Paywall Smasher",
	"version": "2.0.1",
	"description": "Gets around that pesty paywall shhhh",
	"icons": {
		"48": "icons/icon-48.png"
	},
	"content_scripts": [
		{
			"matches": ["*://cooking.nytimes.com/recipes/*", "*://nytimes.com/recipes/*", "*://*.nytimes.com/recipes/*"],
			"js": ["index.js"]
		}
	]
}
