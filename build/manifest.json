{
	"manifest_version": 3,
	"name": "NYT Paywall Smasher",
	"version": "1.0.8",
	"description": "Gets around that pesty paywall shhhh",
	"icons": {
		"48": "icons/icon-48.png"
	},
	"content_scripts": [
		{
			"matches": ["*://cooking.nytimes.com/*", "*://nytimes.com/*", "*://*.nytimes.com/*"],
			"js": ["index.js"]
		}
	]
}
